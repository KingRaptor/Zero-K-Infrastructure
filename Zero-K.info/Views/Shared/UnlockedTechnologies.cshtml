@model IEnumerable<MyController.UnlockCountEntry>
@using ZeroKWeb.Controllers
<div>
@foreach (var au in Model) {
  var unlock = au.Unlock;
  <div style="color: @unlock.LabelColor; font-size:x-small; text-align: center; float: left; width:90px; height:105px; position: relative;" title="$unlock$@unlock.UnlockID"  >
    <img src="@unlock.ImageUrl"/><br />
    @if (au.Count > 1) { 
      <big>@au.Count x</big>
    }
    @unlock.Name
  </div>
}
<br class="clearfloat" />
</div>